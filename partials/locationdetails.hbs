<div>
    <div class="mt-4">
        <h1 class='text-5xl font-extrabold mb-6 text-ll-blue'> {{neighborhood}} Storage Units </h1>
        <h3 class='text-2xl font-extrabold  text-ll-blue'> Local Locker Storage - Discover Storage Near You </h3>
        <div class="reviews-total-count"></div>        
    </div>
    
    <div class="mt-4">
        <div class="grid lg:grid-cols-2 gap-x-4 gap-y-4 grid-cols-1">
            <div class=''>
                <h3 class="font-bold" >Address</h3>
                <p> {{address.line1}} {{address.city}}, {{address.region}} {{address.postalCode}}</p>
            </div>
            <div class=''>
                <h3 class="font-bold" >Phone</h3>
                <p>{{mainPhone}}</p>
            </div>
            <div class=''>
                <h3 class="font-bold" >Access Hours</h3>
                {{>hours hours}}
            </div>
            <div class=''>
                <h3 class="font-bold" >Support Hours</h3>
                {{>hours accessHours}}
            </div>
        </div>
    </div>
</div>