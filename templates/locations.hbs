                                featureType: "water",
                                elementType: "labels.text.stroke",
                                stylers: [{ color: "#17263c" }],
                            },
                            ],

                        disableDefaultUI: true,
                    });

                    var config = {
                      position: mapCenter,
                      map: map
                    };

                    var marker = new google.maps.Marker(config);
                
        });
       </script>     

