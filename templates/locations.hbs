                                            {{mainPhone}}
                                        </div>                                        
                                    </div>                                    
                                </div>
                                <div class="flex flex-row gap-6">
                                    <div class='branch-hours mb-4 mr-32'>
                                        <h3 class="font-bold" >Access Hours</h3>
                                        <div class='hours-list'>
                                            {{#each hours}}
                                                {{#if this.date}}
                                                {{else}}
                                                <p>{{@key}}:
                                                {{#each this}}
                                                    {{#if isClosed}}
                                                    Closed
                                                    {{/if}}
                                                    {{#each this}}
                                                    {{this.start}}-{{this.end}}
                                                    {{/each}}
                                                {{/each}} 
                                                </p> 
                                                {{/if}}
                                            {{/each}}
                                        </div>
                                    </div>
                                    <div class='access-hours mb-4'>
                                        <h3 class="font-bold" >Support Hours</h3>
                                        <div class='hours-list'>
                                            {{#each accessHours}}
                                                {{#if this.date}}
                                                {{else}}
                                                <p>{{@key}}:
                                                {{#each this}}
                                                    {{#if isClosed}}
                                                    Closed
                                                    {{/if}}
                                                    {{#each this}}
                                                    {{this.start}}-{{this.end}}
                                                    {{/each}}
                                                {{/each}} 
                                                </p> 
                                                {{/if}}
                                            {{/each}}
                                        </div>
                                    </div>
                                </div>
                                {{>units}}
                            {{!-- </div> --}}
                        {{!-- </div> --}}
                    </div>

                        <div class="col-span-1 pl-10 lg:col-span-2">
